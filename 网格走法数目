//动态规划   前后有关系，此题中f(x,y) = f(x-1,y) + f(x,y-1)
#include <iostream>
using namespace std;
int main()
{
	int x,y;
	cin >> x >> y;
	int dp[11][11] = {0};
	for(int i = 0; i <= x; i++)
		dp[i][0] = 1;
	for(int i = 0; i <= y; i++)
		dp[0][i] = 1;
	for(int i = 1; i <= x; i++)
	{
		for(int j = 1; j <= y; j++)
		{
			dp[i][j] = dp[i-1][j] + dp[i][j-1];
		}
	}
	cout << dp[x][y] << endl;
	system("pause");
	return 0;
}
